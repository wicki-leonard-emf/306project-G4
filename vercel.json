{
  "version": 2,
  "buildCommand": "cd code/front && npm install && npm run build && cd ../back && npm install",
  "outputDirectory": "code/front/dist",
  "builds": [
    {
      "src": "code/back/src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "code/back/src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}