{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "code/front/build",
  "installCommand": "npm install && cd code/front && npm install && cd ../back && npm install",
  "builds": [
    {
      "src": "code/back/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "code/back/src/index.js"
    },
    {
      "src": "/(?!api).*",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
